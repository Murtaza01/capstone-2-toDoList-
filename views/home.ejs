<%- include('partials/header.ejs')  %>
<div class="container">
    <h1><%= exactDate %></h1>
    <form action="/" method="post">
        <input type="text" name="userText" autocomplete="off">
        <button type="submit">Add</button>
    </form>
    <hr>
    <div class="box">
        <% for(let item of toDoList){%>
        <div class="item">
            <input type="checkbox">
            <p>
                <%= item %>
            </p>
        </div>
        <% } %>
    </div>
</div>

<%- include('partials/footer.ejs')  %>
</body>

</html>